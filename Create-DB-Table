CREATE DATABASE koprol
    WITH
    OWNER = postgres
    ENCODING = 'UTF8'
    LC_COLLATE = 'English_United States.1252'
    LC_CTYPE = 'English_United States.1252'
    TABLESPACE = pg_default
    CONNECTION LIMIT = -1;


CREATE TABLE IF NOT EXISTS public.mstipebarang
(
    idx integer NOT NULL,
    tipeid character(5) COLLATE pg_catalog."default" NOT NULL,
    namatipe character(50) COLLATE pg_catalog."default"
)

TABLESPACE pg_default;

ALTER TABLE IF EXISTS public.mstipebarang
    OWNER to postgres;
